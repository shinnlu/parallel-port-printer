{
  "name": "parallel-port-printer",
  "version": "1.0.7",
  "description": "A Node.js application for controlling parallel port printers",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"nodemon server.js\" \"nodemon server-dev.js\"",
    "start": "node server.js"
  },
  "keywords": [
    "printer",
    "lpt",
    "parallel-port",
    "nodejs"
  ],
  "author": "Shinn Lu",
  "dependencies": {
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "iconv-lite": "^0.6.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "http-proxy-middleware": "^2.0.6",
    "nodemon": "^3.1.10"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "watch": [
    "server.js",
    "routes/",
    "config/"
  ],
  "ext": "js,json",
  "ignore": [
    "node_modules/"
  ],
  "exec": "node server.js"
}
